{
  "name": "react-typescript-seed",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "postcss src/index.css -o src/styles.css",
    "watch:css": "postcss src/index.css -o src/styles.css -w",
    "start": "npm-run-all -p watch:css start-ts",
    "start-ts": "react-scripts-ts start",
    "build-ts": "react-scripts-ts build",
    "build": "npm-run-all build:css build-ts",
    "test": "react-scripts-ts test --env=jsdom --watchAll",
    "test-update": "react-scripts-ts test -u --env=jsdom --watchAll",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "@types/enzyme": "3.1.13",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/jest": "23.3.1",
    "@types/mocha": "5.2.5",
    "@types/node": "10.7.1",
    "@types/react": "16.4.11",
    "@types/react-dom": "16.0.7",
    "@types/react-router": "4.0.30",
    "@types/react-router-dom": "4.3.0",
    "enzyme": "3.4.4",
    "enzyme-adapter-react-16": "1.2.0",
    "mocha": "5.2.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "typescript": "3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "inversify": "4.13.0",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "6.0.0",
    "precss": "3.1.2",
    "react-addons-test-utils": "15.4.2",
    "react-scripts-ts": "2.17.0",
    "reflect-metadata": "0.1.12"
  }
}
